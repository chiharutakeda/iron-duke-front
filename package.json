    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "apollo": "^2.32.8",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "4.2.4"
  }
}
